all: windows mac linux

windows:
	mkdir -p build
	mkdir -p build/windows
	cmake -S . -B build/windows 
	cmake --build build/windows

linux:
	mkdir -p build
	mkdir -p build/linux
	cmake -S . -B build/linux
	cmake --build build/linux

